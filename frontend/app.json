{
  "expo": {
    "name": "Mikel Coffee - Çalışan Sistemi",
    "slug": "mikel-coffee-employee",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mikel-coffee",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mikelcoffee.employee"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#8B4513"
      },
      "package": "com.mikelcoffee.employee",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#8B4513"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "description": "Mikel Coffee çalışanları için kayıt ve yönetim sistemi",
    "primaryColor": "#8B4513"
  }
}
